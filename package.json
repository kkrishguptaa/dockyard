{
  "name": "dockyard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.2.17",
  "engines": {
    "bun": ">=1.2.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "build:json-schema": "bun run src/scripts/build-json-schema.ts",
    "build:api": "bun run src/scripts/build-api.ts",
    "gh-automator": "bun run src/scripts/gh-automator.ts"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.37.0",
    "@github/issue-parser": "2.0.0",
    "@internationalized/date": "3.10.0",
    "@lucide/svelte": "0.545.0",
    "@octokit/rest": "22.0.0",
    "@sveltejs/adapter-static": "3.0.10",
    "@sveltejs/kit": "2.46.5",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.14",
    "@tanstack/table-core": "8.21.3",
    "@types/bun": "1.3.0",
    "@types/frappe-gantt": "0.9.0",
    "bits-ui": "2.11.5",
    "clsx": "2.1.1",
    "eslint": "9.37.0",
    "eslint-plugin-svelte": "3.12.4",
    "formsnap": "2.0.1",
    "frappe-gantt": "1.0.4",
    "globals": "16.4.0",
    "mdsvex": "0.12.6",
    "paneforge": "1.0.2",
    "svelte": "5.39.12",
    "svelte-check": "4.3.3",
    "sveltekit-superforms": "2.27.2",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "tailwindcss": "4.1.14",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1",
    "vite": "7.1.9",
    "vite-plugin-devtools-json": "1.0.0"
  },
  "dependencies": {
    "@sveltejs/adapter-vercel": "^5.7.2",
    "class-variance-authority": "^0.7.1",
    "gsap": "^3.13.0",
    "zod": "^4.0.5"
  }
}
