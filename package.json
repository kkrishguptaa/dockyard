{
  "name": "dockyard",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "packageManager": "bun@1.2.17",
  "engines": {
    "bun": ">=1.2.0"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "build:json-schema": "bun run src/scripts/build-json-schema.ts",
    "build:api": "bun run src/scripts/build-api.ts",
    "gh-automator": "bun run src/scripts/gh-automator.ts"
  },
  "devDependencies": {
    "@eslint/compat": "1.3.1",
    "@eslint/js": "9.32.0",
    "@github/issue-parser": "2.0.0",
    "@lucide/svelte": "0.533.0",
    "@octokit/rest": "22.0.0",
    "@sveltejs/adapter-static": "3.0.8",
    "@sveltejs/kit": "2.26.1",
    "@sveltejs/vite-plugin-svelte": "6.1.0",
    "@tailwindcss/typography": "0.5.16",
    "@tailwindcss/vite": "4.1.11",
    "@types/bun": "1.2.19",
    "clsx": "2.1.1",
    "eslint": "9.32.0",
    "eslint-plugin-svelte": "3.11.0",
    "globals": "16.3.0",
    "mdsvex": "0.12.6",
    "svelte": "5.37.1",
    "svelte-check": "4.3.0",
    "tailwindcss": "4.1.11",
    "typescript": "5.8.3",
    "typescript-eslint": "8.38.0",
    "vite": "7.0.6",
    "vite-plugin-devtools-json": "0.4.1"
  },
  "dependencies": {
    "@sveltejs/adapter-vercel": "^5.7.2",
    "gsap": "^3.13.0",
    "zod": "^4.0.5"
  }
}
